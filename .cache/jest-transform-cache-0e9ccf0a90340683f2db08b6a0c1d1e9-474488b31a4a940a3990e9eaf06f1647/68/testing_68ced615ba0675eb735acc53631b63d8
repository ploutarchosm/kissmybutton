class MockPlatformLocation {
  constructor(config) {
    this.baseHref = "";
    this.hashUpdate = new import_rxjs.Subject();
    this.urlChangeIndex = 0;
    this.urlChanges = [{ hostname: "", protocol: "", port: "", pathname: "/", search: "", hash: "", state: null }];
